{
  "name": "@matrixai/id",
  "version": "3.2.0",
  "author": "Roger Qiu",
  "description": "ID generation for JavaScript & TypeScript Applications",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-id.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -r ./dist || true; tsc -p ./tsconfig.build.json",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "test": "jest",
    "lint": "eslint '{src,tests,benches}/**/*.{js,ts}'",
    "lintfix": "eslint '{src,tests,benches}/**/*.{js,ts}' --fix",
    "docs": "rm -r ./docs || true; typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src && touch ./docs/.nojekyll"
  },
  "dependencies": {
    "multiformats": "^9.4.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.2.5",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.21.5",
    "typescript": "^4.4.3"
  }
}
